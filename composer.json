{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "aws/aws-sdk-php-symfony": "^2.6",
        "bref/bref": "^2.0",
        "bref/symfony-bridge": "^0.2.1",
        "doctrine/dbal": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "phpstan/phpstan": "^1.9",
        "sonata-project/admin-bundle": "^4.13",
        "sonata-project/doctrine-orm-admin-bundle": "^4.3",
        "symfony/amazon-mailer": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/twig-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "bin/phpunit --testdox",
        "sniff": "vendor/bin/phpcs",
        "sniff-fix": "vendor/bin/phpcbf",
        "stan": "vendor/bin/phpstan",
        "migrate": "bin/console doctrine:migrations:migrate --no-interaction"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "6.3.*",
            "allow-contrib": true,
            "docker": false
        }
    }
}
